{
  "name": "ccn-slackbot-hackaton",
  "version": "0.0.0",
  "description": "Slackbot for Hackaton (and maybe other things)",
  "keywords": [
    "Slack",
    "bot",
    "slackbot",
    "collegecryptonetwork",
    "ccn"
  ],
  "repository": {
    "type": "git",
